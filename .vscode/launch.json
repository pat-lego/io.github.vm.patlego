    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Debug CLI Utility",
            "request": "launch",
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "java",
            "name": "Debug (Attach) - Karaf",
            "request": "attach",
            "port": 5005,
             "hostName": "localhost"
        },
        {
            "type": "chrome",
            "name": "Debug (Attach) - Vue.js",
            "request": "launch",
            "url": "http://localhost:8080/profile",
            "breakOnLoad": true,
            "webRoot": "${workspaceFolder}/karaf-system/system.profile/src/main/resources/system.profile/src",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/*",
                "webpack:///*": "*",
                "webpack:///./~/*": "${webRoot}/node_modules/*",
                "meteor://ðŸ’»app/*": "${webRoot}/*"
            }
        }
    ]
}